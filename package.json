{
  "name": "1hr-language-support",
  "displayName": "1hr language support",
  "description": "Simple syntax highlighting for 1hr lang",
  "version": "0.0.1",
  "publisher": "Charles-Benoit",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
"activationEvents": [
    "onLanguage:1hr",
    "onLanguage:xhr",
    "onLanguage:sbb"
],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/DromadaireFache/1hr-language-support.git"
  },
  "contributes": {
    "languages": [
        {
            "id": "1hr",
            "aliases": ["1hr", "1hr"],
            "extensions": [".1hr"],
            "configuration": "./lang_config/1hr-language-configuration.json"
        },
        {
            "id": "xhr",
            "aliases": ["xhr", "xhr"],
            "extensions": [".xhr"],
            "configuration": "./lang_config/xhr-language-configuration.json"
        },
        {
            "id": "sbb",
            "aliases": ["sbb", "sbb"],
            "extensions": [".sbb"],
            "configuration": "./lang_config/sbb-language-configuration.json"
        }
    ],
    "grammars": [
        {
            "language": "1hr",
            "scopeName": "source.1hr",
            "path": "./syntaxes/1hr.tmLanguage.json"
        },
        {
            "language": "xhr",
            "scopeName": "source.xhr",
            "path": "./syntaxes/xhr.tmLanguage.json"
        },
        {
            "language": "sbb",
            "scopeName": "source.sbb",
            "path": "./syntaxes/sbb.tmLanguage.json"
        }
    ]
  }
}
